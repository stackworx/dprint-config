{
  "incremental": true,
  "lineWidth": 80,
  "indentWidth": 2,
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,cs}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/obj/**",
    "**/bin/**",
    "**/dist/**",
    "**/schema.json",
    "**/__generated__/**",
    "**/Migrations/*.cs"
  ],
  "json": {
    "locked": true
  },
  "roslyn": {
    "locked": true,
    "indentWidth": 4
  },
  "typescript": {
    "locked": true,
    "indentWidth": 2,
    "spaceSurroundingProperties": false,
    "importDeclaration.spaceSurroundingNamedImports": false,
    "typeLiteral.spaceSurroundingProperties": false,
    "exportDeclaration.spaceSurroundingNamedExports": false,
    "quoteStyle": "preferSingle",
    "module.sortImportDeclarations": "maintain"
  },
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.59.2.wasm",
    "https://plugins.dprint.dev/json-0.13.2.wasm",
    "https://plugins.dprint.dev/roslyn-0.4.0.exe-plugin@61ad8df289f022d0cd093db6ff48cf9aa4fb22dc1d39ffca7f165c4c376357d4"
  ]
}
